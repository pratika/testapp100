<div class = "custom-title text-center">
	<h1 class="custom-font3">Upload Your Pic</h1>
</div>

<div class = "my-wrapper span4 offset5 text-center">
	<%= simple_form_for @pic  do |f| %>
	<%= f.input :hour, label_html: { class: 'custom-font-form' } %>
	<%= f.input :emotion, label_html: { class: 'custom-font-form' }%>
	<%= f.input :learned, label_html: { class: 'custom-font-form' } %>
	<%= submit_tag "Save", :class => 'btn btn-large btn-warning' %>
<% end %>	
</div>




